{
  "name": "e2e",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "lint": "tslint --project .",
    "build": "tsc --project .",
    "watch": "npm-run-all --parallel \"build -- --watch\" \"cypress:open\"",
    "e2e": "npm-run-all build cypress:run",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run --config video=false --browser chrome",
    "delete-junit-results": "rm build/test-results/test/junit.xml || true",
    "all": "npm-run-all lint build delete-junit-results cypress:run"
  },
  "private": true,
  "dependencies": {
    "rxjs": "^6.3.3"
  },
  "devDependencies": {
    "@types/node": "^8.9.4",
    "cypress": "^3.6.1",
    "npm-run-all": "^4.1.3",
    "rxjs-tslint": "^0.1.5",
    "ts-node": "^7.0.1",
    "tslint": "~5.11.0",
    "tslint-language-service": "^0.9.9",
    "typescript": "~3.1.6"
  }
}
